{
    "name": "agda",
    "displayName": "language-agda",
    "description": "Syntax highlighting and snippets for agda",
    "version": "0.1.5",
    "publisher": "j-mueller",
    "license": "SEE LICENSE IN LICENSE",
    "author": {
        "email": "j.mueller.11@ucl.ac.uk",
        "name": "Jann MÃ¼ller",
        "url": "https://github.com/j-mueller"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/j-mueller/language-agda"
    },
    "bugs": {
        "url": "https://github.com/j-mueller/language-agda/issues"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "keywords": [
        "agda"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "agda",
                "path": "./snippets/snippets.json"
            }
        ],
        "languages": [
            {
                "id": "agda",
                "aliases": [
                    "Agda",
                    "agda"
                ],
                "extensions": [
                    ".agda"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "agda",
                "scopeName": "source.agda",
                "path": "./syntaxes/agda.tmLanguage"
            }
        ]
    }
}